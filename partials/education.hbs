{{#if resume.education.length}}
<section id="education">
	<h2>Education</h2>
	{{#each resume.education}}
	<div class="item">
		{{#if area}}
		<h3 class="area">
			{{area}} 		
			
		</h3>
		{{/if}}
		{{#if institution}}
			<span class="institution">
				{{institution}}
			</span>
		{{/if}}
		<div class="date">
			<small>
			{{#if startDate}}
			<span class="startDate">
				{{startDate}}
			</span>
			{{/if}}
			{{#if endDate}}
			<span class="endDate">
				- {{endDate}}
			</span>
			{{else}}
			<span class="endDate">
				- Present
			</span>
			{{/if}}
			</small>
		</div>

		
		{{#if courses.length}}
		<ul class="courses">
			{{#each courses}}
			<li>{{.}}</li>
			{{/each}}
		</ul>
		{{/if}}
		{{!-- {{#if studyType}}
		<div class="studyType">
			{{studyType}}
		</div>
		{{/if}}
		{{#if gpa}}
		<div class="gpa">
			GPA: {{gpa}}
		</div>
		{{/if}} --}}
	</div>
	{{/each}}
</section>
{{/if}}
